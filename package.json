{
  "name": "game-server-manager",
  "version": "1.0.0",
  "description": "Game server management system for Docker containers",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "build": "cd client && npm run build",
    "prod": "cd server && NODE_ENV=production npm start"
  },
  "keywords": [
    "game",
    "server",
    "docker",
    "management"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "npm": "^11.10.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.1"
  }
}
