    "dotenv": "^16.0.3",
    "express": "^5.2.1",
    "express-fileupload": "^1.5.1",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^9.1.2",
    "node-cron": "^3.0.3",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0"
